mes = "hcszrgpzgphccpxccupkzhypdmhycbopyvqgphyvpsvghopmciparuvprpxccuptycztvptyccgzbxpyzaoprfvbqhpyzgpvmvgpgcpufvramopyzgpyrzfpzgprp'zcbqgparbvo"
mes = mes.lower()
key = 12
alpha = ['a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z','.',' ',"'"]
code = []
i = 0
while i < len(mes):
    code.append(alpha[(alpha.index(mes[i])+key)%len(alpha)])
    i += 1
print(''.join(code))
